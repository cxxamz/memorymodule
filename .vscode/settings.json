{
    "cmake.generator": "Ninja",
    "cmake.buildDirectory": "${workspaceFolder}/.tmp/build",
    "cmake.configureSettings": {
        "CMAKE_EXPORT_COMPILE_COMMANDS": "ON",
        // "CMAKE_TOOLCHAIN_FILE": "${userHome}/env/vcpkg/scripts/buildsystems/vcpkg.cmake",
        "CMAKE_ARCHIVE_OUTPUT_DIRECTORY_DEBUG": "${workspaceFolder}/.tmp/Debug/lib",
        "CMAKE_LIBRARY_OUTPUT_DIRECTORY_DEBUG": "${workspaceFolder}/.tmp/Debug/lib",
        "CMAKE_RUNTIME_OUTPUT_DIRECTORY_DEBUG": "${workspaceFolder}/.tmp/Debug/bin",
        "CMAKE_ARCHIVE_OUTPUT_DIRECTORY_RELEASE": "${workspaceFolder}/.tmp/Release/lib",
        "CMAKE_LIBRARY_OUTPUT_DIRECTORY_RELEASE": "${workspaceFolder}/.tmp/Release/lib",
        "CMAKE_RUNTIME_OUTPUT_DIRECTORY_RELEASE": "${workspaceFolder}/.tmp/Release/bin",
        "CMAKE_ARCHIVE_OUTPUT_DIRECTORY_MINSIZEREL": "${workspaceFolder}/.tmp/MinSizeRel/lib",
        "CMAKE_LIBRARY_OUTPUT_DIRECTORY_MINSIZEREL": "${workspaceFolder}/.tmp/MinSizeRel/lib",
        "CMAKE_RUNTIME_OUTPUT_DIRECTORY_MINSIZEREL": "${workspaceFolder}/.tmp/MinSizeRel/bin",
        "CMAKE_ARCHIVE_OUTPUT_DIRECTORY_RELWITHDEBINFO": "${workspaceFolder}/.tmp/RelWithDebInfo/lib",
        "CMAKE_LIBRARY_OUTPUT_DIRECTORY_RELWITHDEBINFO": "${workspaceFolder}/.tmp/RelWithDebInfo/lib",
        "CMAKE_RUNTIME_OUTPUT_DIRECTORY_RELWITHDEBINFO": "${workspaceFolder}/.tmp/RelWithDebInfo/bin",
    },
    "cmake.configureOnOpen": false,
    "cmake.setBuildTypeOnMultiConfig": true,
    "editor.formatOnSave": false,
    "editor.formatOnPaste": false,
    "editor.rulers": [80,120],
    "files.autoGuessEncoding": true,
    // "editor.codeActionsOnSave": {
    //     "source.organizeImports": "never"
    // },
    // "editor.codeActionsOnSave": {
    //     "editor.formatOnSave": "never",
    //     "source.organizeImports": "never",
    //     "source.sortImports": "never",
    //     "source.fixAll": "never",
    // },
    "glsllint.glslangValidatorArgs": [
        "--target-env", "vulkan1.3"
    ],
    // "glsllint.glslangValidatorPath": "${userHome}/vcpkg/packages/glslang_x64-windows/tools/glslang/glslangValidator.exe",
    "bookmarks.saveBookmarksInProject": true,
    "clangd.arguments": [
        "--compile-commands-dir=${env:RAMDIR}/${workspaceFolderBasename}/build",
        "--clang-tidy",
        "--header-insertion=never",
    ],
    "C_Cpp.errorSquiggles": "disabled",
    "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools",
    "apc.monospace.font.family": "Consolas",
    "apc.font.family": "Consolas",
    "VSCodeCounter.exclude": [
        "**/.rem/**", "**/rem/**",
        "**/.doc/**", "**/doc/**",
        "**/.tmp/**", "**/tmp/**",
        "**/.bin/**", "**/bin/**",
        "**/.out/**", "**/out/**",
        "**/qwindowkit/**",
        "**/Qt-Advanced-Docking-System/**",
        "**/.gitignore",
        "**/.vscode/**",
        "**/node_modules/**"
    ],
    "VSCodeCounter.outputAsCSV": true,
    "VSCodeCounter.outputAsMarkdown": false,
    "VSCodeCounter.outputAsText": false,
    "VSCodeCounter.outputPreviewType": "csv",
    "todo-tree.highlights.enabled": false,
    "todohighlight.isEnable": false,
    "better-comments.tags": [
        {
            "tag": "!",
            "color": "#FF2D00",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "?",
            "color": "#3498DB",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "//",
            "color": "#474747",
            "strikethrough": true,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "todo",
            "color": "#FF8C00",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "*",
            "color": "#98C379",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "BUGFIX",
            "color": "#FFA500",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            // "backgroundColor": "#CD853F",
            "bold": false,
            "italic": false
        },
        {
            "tag": "NOTE",
            "color": "#87CEEB",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "TIPS",
            "color": "#87CEEB",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "FEATURE",
            "color": "#ADFF2F",
            "strikethrough": false,
            "underline": false,
            // "backgroundColor": "#2c2c2c7f",
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        }
    ],
